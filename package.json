{
  "name": "switcher",
  "version": "1.0.0",
  "description": "Switch between development environments",
  "author": "Biel Simon",
  "license": "MIT",
  "keywords": [
    "ide",
    "automation",
    "typescript"
  ],
  "homepage": "https://github.com/biels/switcher",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/biels/switcher.git"
  },
  "bugs": {
    "url": "https://github.com/biels/switcher/issues"
  },
  "main": "main-process.js",
  "scripts": {
    "test": "npx jest",
    "build": "npx webpack",
    "build:prod": "npx webpack --env production",
    "start": "npx concurrently \"npx webpack serve --config-name renderer --env hot-reload\" \"npx webpack --config-name main && npx electron ./dist/main-process.js\" --kill-others --kill-others-on-fail",
    "start:prod": "npm run build:prod && npx electron ./dist/main-process.js",
    "pack": "npx rimraf ./pack && npm run build:prod && npx electron-packager ./dist --out ./pack --overwrite",
    "dist": "yarn build:prod && electron-builder -w --publish never"
  },
  "dependencies": {
    "@electron/remote": "^2.0.1",
    "@fontsource/roboto": "^4.5.1",
    "cuid": "^2.1.8",
    "electron-store": "^8.0.1",
    "encoding": "^0.1.13",
    "json5": "^2.2.0",
    "mobx": "^6.3.13",
    "mobx-react": "^7.2.1",
    "mobx-react-form": "^3.2.0",
    "moment": "^2.29.1",
    "node-powershell": "^5.0.1",
    "react": "^17.0.2",
    "react-beautiful-dnd": "^13.1.0",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-router-dom": "^6.2.1",
    "reflect-metadata": "^0.1.13",
    "styled-components": "^5.3.3",
    "ts-loader": "^9.2.6",
    "tsyringe": "^4.6.0",
    "zx": "^4.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.16.7",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.4",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.8",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.3.2",
    "@types/styled-components": "^5.1.19",
    "@types/webpack": "^5.28.0",
    "@types/webpack-dev-server": "^4.7.2",
    "@types/webpack-env": "^1.16.3",
    "babel-loader": "^8.2.3",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^7.0.0",
    "copy-webpack-plugin": "^10.2.0",
    "electron": "^16.0.6",
    "electron-builder": "^22.14.5",
    "electron-packager": "^15.4.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^27.4.7",
    "react-refresh": "^0.11.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.5.4",
    "webpack": "^5.65.0",
